    <style>
        .head span {
            font-size: 50px;
            /* text-shadow: 0px 0px 18px red; */
            line-height: 58px;
            margin: 0px -5px;
        }

        .head {
            padding-top: 100px;
            display: flex;
            align-items: baseline;
            transform: skewy(-3.5deg);
            color: rgb(255, 255, 255);
            font-weight: bold;
            font-size: 20px;
            display: flex;
            justify-content: center;
        }

        .cont {
            padding-top: 5rem;
            width: 100%;
            height: 249px;
            color: white;
            transform: skewy(3.5deg) translatey(-81px);
            padding-top: 2.5rem;
            background-color: #7d01011c;
            border-bottom: 2px solid #ff00005e;
        }

        .teaminfo {
            padding: 12px;
            display: flex;
            min-width: 25%;
            width: auto;
            flex-direction: column;
            justify-content: center;
            height: 300px;
            margin: 0px 200px 100px 100px;
            border: 2px solid white;
            border-radius: 30px;
            backdrop-filter: blur(10px) saturate(200%);
            /* box-shadow: 23px -16px 82px -39px #ff0000; */
            text-align: center;
            position: fixed;
            align-items: center;
            max-width: 30%;
        }

        .teaminfo span {
            display: flex;
            margin: 3px;
            align-items: center;
            justify-content: center;
            color: white;
            font-size: 18px;
            height: 20%;
        }

        .player {
            border: 2px solid #ffffff80;
            /* position: absolute; */
            margin: 10px;
            width: auto;
            height: 90px;
            /* min-width: 26%; */
            min-height: 90px;
            display: flex;
            flex-direction: column;
            align-items: center;
            justify-content: center;
            color: white;
            font-size: 17px;
            border-radius: 30px;
            backdrop-filter: blur(10px) saturate(120%);
            box-shadow: 0px 0px 18px -7px #ff0000;
        }
    </style>


